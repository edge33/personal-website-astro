---
import plainwhiteConfig from '../plainwhite.config';
import DarkMode from './DarkMode/DarkMode.astro';

interface Props {
    condensedClass: string;
}

const { condensedClass } = Astro.props;

const {
    plainwhite: {
        dark_mode,
        social_links,
        // navigation
    },
} = plainwhiteConfig;

const {
    linkedIn,
    twitter,
    github,
    email,
    flickr,
    gitlab,
    facebook,
    instagram,
    pinterest,
    soundcloud,
    youtube,
    telegram,
    dribbble,
    stackoverflow,
} = social_links as { [key: string]: string };
